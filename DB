-- Dumping structure for table qcu_attendance.schedule
CREATE TABLE IF NOT EXISTS `schedule` (
  `Schedule_id` int(11) NOT NULL AUTO_INCREMENT,
  `Employee_code` int(11) DEFAULT NULL,
  `Workdays` varchar(50) DEFAULT NULL,
  `shift_start_time` varchar(50) DEFAULT NULL,
  `shift_end_time` varchar(50) DEFAULT NULL,
  `Schedule_start_date` varchar(50) DEFAULT NULL,
  `Schedule_end_date` varchar(50) DEFAULT NULL,
  PRIMARY KEY (`Schedule_id`),
  KEY `Employee_code` (`Employee_code`),
  CONSTRAINT `FK_schedule_employee` FOREIGN KEY (`Employee_code`) REFERENCES `employee` (`Employee_code`) ON DELETE NO ACTION ON UPDATE NO ACTION
