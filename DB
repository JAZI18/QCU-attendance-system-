-- Dumping structure for table qcu_attendance.employee
CREATE TABLE IF NOT EXISTS `employee` (
  `Employee_id` int(11) NOT NULL AUTO_INCREMENT,
  `Employee_code` int(11) DEFAULT NULL,
  `Gender` varchar(50) DEFAULT NULL,
  `Address` varchar(50) DEFAULT NULL,
  `DOB` varchar(50) DEFAULT NULL,
  `Branch` varchar(50) DEFAULT NULL,
  `Department` varchar(50) DEFAULT NULL,
  PRIMARY KEY (`Employee_id`),
  KEY `Employee_code` (`Employee_code`),
  CONSTRAINT `FK__administrator` FOREIGN KEY (`Employee_id`) REFERENCES `administrator` (`Employee_id`) ON DELETE NO ACTION ON UPDATE NO ACTION,
  CONSTRAINT `FK_employee_schedule` FOREIGN KEY (`Employee_code`) REFERENCES `schedule` (`Employee_code`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

